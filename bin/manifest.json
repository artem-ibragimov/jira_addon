{
    "manifest_version": 3,
    "author": "artem.ibragimov",
    "name": "Jira Add-on",
    "description": "Jira Extension",
    "version": "0.0.1",
    "host_permissions": [
        "*://*.atlassian.net/*"
    ],
    "icons": {
        "16": "./asset/i16.png",
        "32": "./asset/i32.png",
        "48": "./asset/i48.png",
        "128": "./asset/i128.png",
        "225": "./asset/i225.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.atlassian.net/*"
            ],
            "all_frames": true,
            "js": [
                "main.js"
            ]
        }
    ]
}